		"webpack": "^4.42.0",
		"xterm": "^4.6.0"
	},
	"build": {
		"appId": "test",
		"extraFiles": [
			"python_src/*",
			"src/**/*"
		],
		"linux": {
			"target": [
				"AppImage",
				"dir"
			],
			"executableName": "test"
		},
		"asar": true
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"rebuild": "electron-rebuild",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"electronb": "electron ./build/electron.js",
		"electron": "electron ./public/electron.js",
		"dev": "NODE_ENV='development' nf start",
		"pythonrun": "~/miniconda3/envs/mp_test/bin/python ./python_src/run.py",
		"dist:linux": "electron-builder  --projectDir ./ --linux"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@prettier/plugin-python": "0.0.0-development",
		"@types/jest": "^26.0.10",
		"@types/node": "^14.6.1",
		"@types/react": "^16.9.48",
		"@types/react-dom": "^16.9.8",
		"electron": "^18.3.7",
		"electron-builder": "^22.8.0",
		"electron-rebuild": "^2.0.1",
		"foreman": "^3.0.1"
	}
